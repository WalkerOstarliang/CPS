	<display-name></display-name>
	<tlib-version>2.2</tlib-version>
	<short-name>cps</short-name>
	<uri>/tags/cps</uri>
	<!-- 带标题的div
		title：标题
		cssStyle：行内样式
		cssClass：class
		width：宽度
		height：高度
		scroll：滚动条
	 -->
	<tag>
		<name>panel</name>
		<tag-class>com.chinacreator.common.taglib.tag.PanelTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cssStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>scroll</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>mode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[
					mode : view
				]]>
			</description>
		</attribute>
	</tag>
	
	<!-- 
		在panel中添加按钮 
	 -->
	<tag>
		<name>tbar</name>
		<tag-class>com.chinacreator.common.taglib.tag.TbarTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<!-- panel容器
		 与 panel 、tbar 标签一起使用 实现在panel标题中添加按钮
	 -->
	<tag>
		<name>PanelView</name>
		<tag-class>com.chinacreator.common.taglib.tag.PanelViewTag</tag-class>
		<body-content>JSP</body-content>
